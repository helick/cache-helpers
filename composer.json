{
    "name": "helick/cache-helpers",
    "type": "library",
    "description": "Helpers for the WordPress object cache and transients",
    "keywords": [
        "helick",
        "wordpress",
        "cache",
        "helpers"
    ],
    "license": "MIT",
    "support": {
        "issues": "https://github.com/helick/cache-helpers/issues",
        "source": "https://github.com/helick/cache-helpers"
    },
    "authors": [
        {
            "name": "Evgenii Nasyrov",
            "email": "evgenii@helick.io"
        }
    ],
    "require": {
        "php": ">=7.1",
        "composer/installers": "^1.6"
    },
    "require-dev": {
        "squizlabs/php_codesniffer": "^3.4"
    },
    "autoload": {
        "files": [
            "src/helpers.php"
        ]
    },
    "scripts": {
        "lint": [
            "phpcs -p --standard=psr2 src"
        ]
    },
    "config": {
        "sort-packages": true
    }
}
